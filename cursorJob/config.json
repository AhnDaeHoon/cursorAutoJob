{
  "description": "Cursor IDE AI 자동화 설정",
  "version": "1.2",
  "activation_delay": 1.0,
  "keystroke_delay": 1.0,
  "enter_delay": 0.5,
  "final_delay": 2.0,
  "chat_focus_enabled": true,
  "chat_click_delay": 0.5,
  "chat_click_coordinates": [400, 700],
  "fallback_keyboard_shortcut": "Cmd+L",
  "commands": [
    {
      "interval": 600,
      "max_count": 1,
      "command": "@direction_1.md"
    },
    {
      "interval": 600,
      "max_count": 1,
      "command": "@direction_2.md"
    },
    {
      "interval": 600,
      "max_count": 1,
      "command": "@direction_3.md"
    }
  ]
}
